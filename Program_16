// Program 16: Prime Number.
#include <stdio.h>

void main(){
    int n,i,flag=0;
    printf("enter n:");
    scanf("%d",&n);
    for (i=2; i<n; i++){
        if (n%i!=0){
            continue;
        } else {
            flag==1;
            printf("not a prime number.");
        }
    } if (flag==0){
        printf("Prime number.");
    }
}

--- Output:
 enter n:17
prime number,
