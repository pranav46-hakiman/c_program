Program 21: 2D - Array

#include <stdio.h>

void main(){
    int i, j, arr[2][2];
    for (i=0; i<2; i++){
        for (j=0; j<2; j++){
            printf("enter element:");
            scanf("%d",&arr[i][j]);
        }
    }
     for (i=0; i<2; i++){
        for (j=0; j<2; j++){
            printf(" %d",arr[i][j]);
        }
    }printf("\n");

}
